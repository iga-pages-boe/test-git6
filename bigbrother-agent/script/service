[Unit]
Description=${Name}@${Version}
After=network-online.target

[Service]
Type=simple
LimitNOFILE=1024
User=root
Group=root
EnvironmentFile=/etc/default/dsa_env
WorkingDirectory=${WorkingDirectory}
ExecStart=${WorkingDirectory}bin/$RUN_NAME $RUN_NAME
Restart=always

[Install]
WantedBy=default.target